{
    "name": "bitbucket-copy-format",
    "version": "2.0",
    "permissions": [
      "activeTab",
      "clipboardWrite"
    ],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_end"
    }],
    "browser_action": {
      "default_title": "Copy commits and format to redmine format"
    },
    "manifest_version": 2
  }